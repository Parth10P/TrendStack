{
  "expo": {
    "name": "TrendStack",
    "slug": "TrendStack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.597436326919-1e131906c87e6dkkesmcee2sdgta90lb"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifire": "com.TrendStack.ios",
      "bundleIdentifier": "com.TrendStack.android"
    },
    "android": {
      "package": "com.TrendStack.android",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
